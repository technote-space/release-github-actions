{
  "name": "@technote-space/github-action-helper",
  "version": "5.2.11",
  "description": "Helper for GitHub Action.",
  "keywords": [
    "github",
    "github actions"
  ],
  "homepage": "https://github.com/technote-space/github-action-helper",
  "bugs": {
    "url": "https://github.com/technote-space/github-action-helper/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/technote-space/github-action-helper.git"
  },
  "license": "MIT",
  "author": {
    "name": "Technote",
    "email": "technote.space@gmail.com",
    "url": "https://technote.space"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "cover": "jest --coverage",
    "lint": "eslint 'src/**/*.ts' '__tests__/**/*.ts' --cache",
    "lint:fix": "eslint --fix 'src/**/*.ts' '__tests__/**/*.ts'",
    "test": "yarn lint && yarn cover",
    "update": "npm_config_yes=true npx npm-check-updates -u --timeout 100000 && yarn install && yarn upgrade && yarn audit",
    "postinstall": "[ -n \"$CI\" ] || [ ! -f node_modules/.bin/husky ] || husky install",
    "prepublishOnly": "[ -n \"$CI\" ] || [ ! -f node_modules/.bin/pinst ] || pinst --disable",
    "postpublish": "[ -n \"$CI\" ] || [ ! -f node_modules/.bin/pinst ] || pinst --enable"
  },
  "dependencies": {
    "@actions/core": "^1.4.0",
    "@actions/github": "^5.0.0",
    "@octokit/openapi-types": "^8.2.0",
    "@octokit/plugin-rest-endpoint-methods": "^5.4.0",
    "@technote-space/github-action-log-helper": "^0.1.30",
    "shell-escape": "^0.2.0",
    "sprintf-js": "^1.1.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@technote-space/github-action-test-helper": "^0.7.16",
    "@types/jest": "^26.0.23",
    "@types/node": "^16.0.0",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "@typescript-eslint/parser": "^4.28.1",
    "eslint": "^7.30.0",
    "husky": "^7.0.0",
    "jest": "^27.0.6",
    "jest-circus": "^27.0.6",
    "lint-staged": "^11.0.0",
    "nock": "^13.1.0",
    "ts-jest": "^27.0.3",
    "typescript": "^4.3.5"
  },
  "publishConfig": {
    "access": "public"
  }
}
